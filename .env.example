# Database Configuration
# If using Postgres (Recommended for production):
# DATABASE_URL=postgres://user:password@localhost:5432/codestream
# If not provided, it defaults to SQLite (database.sqlite)

# Session Secret (Change this for production!)
SESSION_SECRET=supersecretkey_change_me

# Authentication - GitHub (https://github.com/settings/applications/new)
# Callback URL: http://localhost:3000/auth/github/callback
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Authentication - Google (https://console.cloud.google.com/)
# Enable "Google+ API" or "Google People API"
# Callback URL: http://localhost:3000/auth/google/callback
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Judge0 API Key (https://rapidapi.com/judge0-official/api/judge0-ce)
# Required for code execution
API_KEY=your_rapidapi_key

# Google Gemini API Key (https://aistudio.google.com/)
# Required for AI Assistant
GOOGLE_API_KEY=your_gemini_api_key

# Port
PORT=3000
